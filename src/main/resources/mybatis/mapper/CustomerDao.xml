<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="app.foodylab.adaptor.infra.mapper.CustomerMapper">

  <insert id="insertCustomer" parameterType="app.foodylab.domain.customer.Customer">
    INSERT INTO customer
      (name)
    VALUES
      (#{name});
  </insert>
</mapper>